QTableView
{
    color: green;                                       /*表格内文字颜色*/
    gridline-color: rgb(108,108,108);                              /*表格内框颜色*/
    background-color: rgb(0,0,0);               /*表格内背景色*/
    alternate-background-color: rgb(5, 5, 5);
    selection-color: white;                             /*选中区域的文字颜色*/
    selection-background-color: rgb(15, 15, 15);        /*选中区域的背景色*/
	border: none;
	padding :0px 0px;
	font:  18px "黑体";
}

QScrollBar:vertical
{
    width:20px;
    background:rgba(0,0,0,0%);
    margin:0px,0px,0px,0px;
	padding-top:20px; 
    padding-bottom:20px;
}
QScrollBar::handle:vertical
{
    width:20px;
    background:rgba(90,90,90,25%);
    border-radius:4px;   
    min-height:20;
}
QScrollBar::handle:vertical:hover
{
    width:20px;
    background:rgba(90,90,90,50%);  
    border-radius:4px;
    min-height:20;
}
QScrollBar::add-line:vertical  
{
    height:20px;
	width:20px;
    subcontrol-position:bottom;
	border-image:url(:/res/pic/downl.png);
}
QScrollBar::sub-line:vertical  
{
    height:20px;
	width:20px;
    subcontrol-position:top;
	border-image:url(:/res/pic/upl.png);
}
QScrollBar::add-line:vertical:hover   
{
    height:20px;
	width:20px;
    subcontrol-position:bottom;
	border-image:url(:/res/pic/down.png);
}
QScrollBar::sub-line:vertical:hover  
{
    height:20px;
	width:20px;
    subcontrol-position:top;
	border-image:url(:/res/pic/up.png);
}
QScrollBar::add-page:vertical,QScrollBar::sub-page:vertical 
{
    background:rgba(0,0,0,10%);
    border-radius:4px;
}

QScrollBar:horizontal
{
    height:20px;
    background:rgba(0,0,0,0%);
    margin:0px,0px,0px,0px;
	padding-left:20px; 
    padding-right:20px;
}
QScrollBar::handle:horizontal
{
    height:20px;
    background:rgba(90,90,90,25%);
    border-radius:4px;   
    min-width:20;
}
QScrollBar::handle:horizontal:hover
{
    height:20px;
    background:rgba(90,90,90,50%);  
    border-radius:4px;
    min-width:20;
}
QScrollBar::add-line:horizontal  
{
    height:20px;
	width:20px;
    subcontrol-position:right;
	border-image:url(:/res/pic/rightl.png);
}
QScrollBar::sub-line:horizontal  
{
    height:20px;
	width:20px;
    subcontrol-position:left;
	border-image:url(:/res/pic/leftl.png);
}
QScrollBar::add-line:horizontal:hover   
{
    height:20px;
	width:20px;
    subcontrol-position:right;
	border-image:url(:/res/pic/rightd.png);
}
QScrollBar::sub-line:horizontal:hover  
{
    height:20px;
	width:20px;
    subcontrol-position:left;
	border-image:url(:/res/pic/leftd.png);
}
QScrollBar::add-page:horizontal,QScrollBar::sub-page:horizontal 
{
    background:rgba(0,0,0,10%);
    border-radius:4px;
}