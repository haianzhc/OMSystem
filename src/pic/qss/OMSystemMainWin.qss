QMenuBar
{
	background-color: rgb(53, 53, 53);
}

QMenuBar::item 
{
    background-color: transparent;
	color:white;
	padding:8px 16px;/*设置菜单项文字上下和左右的内边距，效果就是菜单中的条目左右上下有了间隔*/
}
  
QMenuBar::item:selected 
{ 
    background-color: rgb(90, 90, 90);
}

QMenu 
{
    background-color: rgb(90, 90, 90); 
}
  
QMenu::item
{
    background-color: transparent;
	color:white;
    padding:8px 32px;/*设置菜单项文字上下和左右的内边距，效果就是菜单中的条目左右上下有了间隔*/
    margin:0px 8px;/*设置菜单项的外边距*/
}
  
QMenu::item:selected 
{
    background-color: rgb(0, 122, 204);/*这一句是设置菜单项鼠标经过选中的样式*/
}

QMenuBar::separator
{

 	background:rgb(120,120,120);

}

QToolBar::separator
{
 	background:rgb(120,120,120);
	width:2px;
	height:1px;
}

QToolBar
{
 	background-color:rgb(53, 53, 53);
	border: 0px;
	spacing:10;
}

QStatusBar
{		
	background-color: rgb(0, 122, 204);
}

QStatusBar::item
{	
	background-color: transparent;
}

QGroupBox 
{
	border: 2px solid rgb(90, 90, 90);
	border-radius: 5px;
	margin-top: 1ex; 
	font: 12pt "黑体";
}

QGroupBox::title 
{
	color:white;
}

#OMSystemMainWin
{
	background-color: rgb(53, 53, 53);
	color: rgb(255, 255, 255);
}

QProgressBar
{
    border: none;
    color: white;
    text-align: center;
    background: rgb(90, 90, 90);
    border-radius:5px;
}

QProgressBar::chunk 
{
    border: none;
    background: rgb(85, 170, 127);
    border-radius:5px;
}

QToolButton
{  
	background-color: transparent;
    color:white;
    font: 10pt "黑体";
}  

QToolButton:hover
{  
	background: rgb(90, 90, 90); 
}  
QToolButton:pressed
{ 
	background: rgb(90, 90, 90); 
}  
QToolButton:checked
{    
	background: rgb(90, 90, 90);   
}

